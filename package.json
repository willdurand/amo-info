{
  "name": "amo-info",
  "version": "1.0.0",
  "description": "An extension to show information about AMO related services.",
  "author": "William Durand <will+git@drnd.me>",
  "license": "MPL-2.0",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build:release": "npm run build && rimraf dist-zip/ && node scripts/build-zip.js",
    "lint": "eslint --ext .js,.vue src",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "rimraf": "^2.6.2",
    "string-humanize": "^1.0.1",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.4.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-shell-plugin": "^0.5.0",
    "zip-folder": "^1.0.0"
  }
}
